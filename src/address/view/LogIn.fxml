<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="address.view.LogInController">
   <children>
      <Label layoutX="216.0" layoutY="83.0" text="Carloan" textFill="#65ab7a" AnchorPane.leftAnchor="216.0" AnchorPane.rightAnchor="237.0">
         <font>
            <Font size="64.0" />
         </font>
      </Label>
      <ButtonBar layoutX="493.0" layoutY="343.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#handleRegister" text="Register" />
        </buttons>
      </ButtonBar>
      <GridPane layoutX="144.0" layoutY="195.0" prefHeight="77.0" prefWidth="426.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="225.0" minWidth="10.0" prefWidth="188.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="240.0" minWidth="10.0" prefWidth="238.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label prefHeight="16.0" prefWidth="198.0" text="Username or email address:" />
            <Label text="Password:" GridPane.rowIndex="1" />
            <TextField fx:id="usernameField" onAction="#handleLogIn" prefWidth="214.0" GridPane.columnIndex="1" />
            <PasswordField fx:id="passwordField" onAction="#handleLogIn" onMouseClicked="#handleMouseClick" text="password" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button layoutX="508.0" layoutY="272.0" mnemonicParsing="false" onAction="#handleLogIn" text="Log In" />
   </children>
</AnchorPane>
